{
  "name": "math-exam-splitter",
  "version": "1.12.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "jszip": "^3.10.1",
    "katex": "^0.16.9",
    "pdfjs-dist": "^4.0.379",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^9.0.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm --no-git-tag-version version minor",
    "deploy": "vite build && wrangler deploy",
    "postdeploy": "yarn commit",
    "commit": "git add . && git commit -m 'chore: update package version' && git push",
    "db": "wrangler d1 execute gksx --yes --file=migrations/tmp.sql",
    "db:init:remote": "wrangler d1 execute gksx --file=schema.sql --remote",
    "db:init": "wrangler d1 execute gksx --local --file=schema.sql"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.21.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^6.4.1",
    "wrangler": "^4.60.0"
  }
}
